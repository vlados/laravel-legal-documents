{
    "name": "vlados/laravel-legal-documents",
    "description": "Track legal document versions and user acceptances in Laravel applications",
    "keywords": [
        "laravel",
        "legal",
        "documents",
        "gdpr",
        "terms",
        "privacy",
        "acceptance",
        "versioning"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vlado",
            "email": "vlado@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "spatie/laravel-translatable": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0"
    },
    "suggest": {
        "filament/filament": "Required for Filament admin panel integration (^3.0|^4.0)",
        "lara-zeus/spatie-translatable": "Required for Filament translatable support (^1.0)"
    },
    "autoload": {
        "psr-4": {
            "Vlados\\LegalDocuments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vlados\\LegalDocuments\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vlados\\LegalDocuments\\LegalDocumentsServiceProvider"
            ],
            "aliases": {
                "LegalDocuments": "Vlados\\LegalDocuments\\Facades\\LegalDocuments"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
